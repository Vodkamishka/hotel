mixin demo ({link, websiteTitles, uikitTitles})
    .demo
        .demo__see See demo
        .demo__title Website Pages
        for el in websiteTitles
            a(href=`${link}/${el.link}.html` target="_blank")= el.title
        .demo__title Ui Kit 
        for el in uikitTitles
            a(href=`${link}/${el.link}.html` target="_blank")= el.title   