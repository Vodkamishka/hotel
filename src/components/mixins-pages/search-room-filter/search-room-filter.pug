include ../../common/footer/footer
include ../../common/copyright/copyright
include ../../common/header/header
include ./search-room-filter-column/search-room-filter-column
include ./search-room-filter-images/search-room-filter-images

mixin search-room-filter (options)
    - const {header, footer, copyright} = options
    .search-room-filter
        +header(header)
        .search-room-filter__content
            .search-room-filter__wrapper-column
                +search-room-filter-column(options)
            .search-room-filter__wrapper-images
                +search-room-filter-images
        +footer(footer)
        +copyright(copyright)

